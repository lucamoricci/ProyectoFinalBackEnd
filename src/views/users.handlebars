<h1>Lista de usuarios: </h1>

<table class="table table-striped">
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Email</th>
            <th>Rol</th>
            <th>Cambiar rol</th>
        </tr>
    {{#each users}}
    <tr>
        <td>{{this._id}}</td>
        <td>{{this.name}}</td>
        <td>{{this.lastname}}</td>
        <td>{{this.email}}</td>
        <td>{{this.profile}}</td>
        <th><button id="toUser" data-id="{{this._id}}" type="button" class="btn btn-primary btn-toUser">user</button>
            <button id="toPremium" data-id="{{this._id}}" type="button" class="btn btn-success btn-toPremium">premium</button>
            <button id="toAdmin" data-id="{{this._id}}" type="button" class="btn btn-info btn-toAdmin">admin</button></th>
        
    </tr>    
    {{/each}}
</table>


<script src="/js/vistaUsers.js" type="module"></script>

